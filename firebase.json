{
  "hosting": {
    "public": "main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
  "extensions": {
    "firestore-stripe-payments": "stripe/firestore-stripe-payments@0.3.3"
  }
}
